{
  "name": "react-date-select",
  "version": "0.0.6",
  "description": "A Date Select / Picker Input Component built with and for ReactJS",
  "main": "lib/DateSelect.js",
  "author": "Jed Watson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JedWatson/react-date-select.git"
  },
  "dependencies": {
    "blacklist": "^1.1.2",
    "classnames": "^2.1.1",
    "moment": "~2.10.3"
  },
  "devDependencies": {
    "babel": "^5.4.3",
    "gulp": "^3.8.11",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^0.3.0",
    "react": ">=0.12.0",
    "react-component-gulp-tasks": "^0.7.0"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "browserify-shim": {
    "blacklist": "global:blacklist",
    "classnames": "global:classNames",
    "moment": "global:moment",
    "react": "global:React"
  },
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build",
    "bump": "gulp bump",
    "bump:major": "gulp bump:major",
    "bump:minor": "gulp bump:minor",
    "dev": "gulp dev",
    "examples": "gulp dev:server",
    "lint": "eslint ./; true",
    "release": "gulp release",
    "publish:site": "gulp publish:examples",
    "test": "mocha --compilers js:babel/register",
    "watch": "gulp watch:lib"
  },
  "keywords": [
    "react",
    "react-component",
    "date",
    "datepicker",
    "dateinput",
    "input",
    "form",
    "ui"
  ]
}
